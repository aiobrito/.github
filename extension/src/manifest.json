{
  "manifest_version": 3,
  "name": "AIOPY Notion Extension",
  "version": "2.0",
  "description": "Extens√£o para capturar dados e integrar com o Notion",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
